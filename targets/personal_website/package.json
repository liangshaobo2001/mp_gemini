{"name": "waa-workspace", "version": "1.0.0", "main": "index.js", "scripts": {"start": "nodemon index.js > .waa/server.log 2>&1 &", "start:sync": "nodemon index.js", "stop": "pkill -f 'node.*index.js' || true", "logs": "tail -n 20 .waa/server.log", "logs:follow": "tail -f .waa/server.log", "clean": "rm -rf .waa/server.log", "dev": "nodemon index.js", "status": "pgrep -f 'node.*index.js' && echo 'Server is running' || echo 'Server is not running'"}, "author": "", "license": "ISC", "dependencies": {"express": "^4.18.2", "express-handlebars": "^7.1.2"}, "devDependencies": {"nodemon": "^3.0.1"}}